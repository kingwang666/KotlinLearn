<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.wang.kotlindemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".activity.KotlinBaseActivity"/>
        <activity android:name=".activity.ClassExtendsActivity"/>
        <activity android:name=".activity.FieldActivity"/>
        <activity android:name=".activity.ExtendActivity"/>
        <activity android:name=".activity.ObjectActivity"/>
        <activity android:name=".activity.ByActivity"/>
        <activity android:name=".activity.FunActivity"/>
        <activity android:name=".activity.CoroutinesActivity"/>
    </application>

</manifest>